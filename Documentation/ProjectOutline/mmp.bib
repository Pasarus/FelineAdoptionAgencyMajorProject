    Author = {Aaron Parecki},
    Organisation = {},
    Howpublished = {\url{https://oauth.net/2/native-apps/}},
    Note = {Accessed 05/02/2020},
    Title = {{RFC 8252: OAuth 2.0 for Mobile and Native Apps}}
}

